@model IEnumerable<I_Facility.Models.OperatorDashboard>
@{
    ViewBag.Title = "Virtual HMI";
    Layout = "~/Views/Shared/_OperatorLayout.cshtml";
    var slno = 1;
}
<input id="SetupError" value="@ViewBag.SetupError" type="hidden" />
<div class="">
    <div class="x_panel">


        <div class="x_content">
            <table id="datatable-fixed-header" class="table table-striped table-bordered dt-responsive nowrap" cellspacing="0" width="100%">
                <thead>
                    <tr>
                        <th>#</th>
                        <th>Status</th>
                        <th>Machine Name</th>
                        <th>Description</th>
                        <th>Start Time</th>
                        <th>End Time</th>
                        <th>Total Duration (Mins)</th>
                    </tr>
                </thead>
                <tbody>
                    @if (Model != null)
                    {
                        foreach (var item in Model)
                        {
                            //foreach (var item1 in item.OpDashboardList)
                            //{
                            <tr>
                                <td>@slno</td>
                                <td>@Html.DisplayFor(modelItem => item.OpDashboard.MessageCode)</td>
                                <td>@Html.DisplayFor(modelitem => item.machinename)</td>
                                <td>@Html.DisplayFor(modelItem => item.OpDashboard.MessageDescription)</td>
                                <td>@Html.DisplayFor(modelItem => item.OpDashboard.MessageStartTime)</td>
                                @if (item.OpDashboard.MessageEndTime.HasValue)
                                {
                                    <td>@Convert.ToDateTime(item.OpDashboard.MessageEndTime).ToString("dd-MM-yyyy HH:mm:ss")</td>
                                }
                                else
                                {
                                    <td></td>
                                }
                                <td>@Html.DisplayFor(modelItem => item.OpDashboard.TotalDurationinMin)</td>
                            </tr>
                            slno++;
                            //}
                        }
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>